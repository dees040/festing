{
    "name": "dees040/festing",
    "description": "Fasten up your unit tests in Laravel by more than 100%",
    "keywords": [
        "Laravel",
        "Unit",
        "Tests",
        "Fast"
    ],
    "authors": [
        {
            "name": "Dees Oomens",
            "email": "dees@owow.io"
        }
    ],
    "require": {
        "illuminate/container": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*"
    },
    "autoload": {
       "psr-4": {
            "Dees040\\Festing\\": "src/Festing"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dees040\\Festing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dees040\\Festing\\ServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
